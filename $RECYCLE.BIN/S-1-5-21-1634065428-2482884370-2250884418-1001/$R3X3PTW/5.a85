 ; program to multiply two 8 bit numbers to give a 16 bit value

  KickOff 100h
  Org 100h ; This is where the control is first transferred
     
          mvi b,8
          mvi c,3
          mov a,c
          cpi 0
          jz last
          dcr c
          mov a,b
          mvi d,0
  loop:   add b
          jnc next
          inr d
  next:   dcr c
          jnz loop
  last:   hlt

